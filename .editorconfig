[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
max_line_length = 120
tab_width = 4
trim_trailing_whitespace = true
ij_visual_guides = 79,120

[{*.bash,*.sh,*.zsh}]
indent_size = 2
tab_width = 2

[*.json]
indent_size = 2

[{*.yaml,*.yml}]
indent_size = 2
